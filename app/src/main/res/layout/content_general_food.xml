<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.kevmc.kcalcount.GeneralFood"
    tools:showIn="@layout/activity_general_food2">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="81dp"
        android:text="General Food Page"
        android:textSize="24dp"
        tools:layout_editor_absoluteX="133dp"
        tools:layout_editor_absoluteY="158dp" />

    <Button
        android:id="@+id/btn_find_food"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/et_food_to_search"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="41dp"
        android:onClick="findFood"
        android:text="Find Food" />

    <EditText
        android:id="@+id/et_food_to_search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignStart="@+id/textView4"
        android:layout_below="@+id/textView4"
        android:layout_marginTop="92dp"
        android:ems="10"
        android:hint="Enter food name..."
        android:inputType="textPersonName" />



</RelativeLayout>
